{
    "name": "uchm4n/skeleton-package-package",
    "description": "This is my package skeleton-package",
    "keywords": [
        "uchm4n",
        "laravel",
        "skeleton-package-package"
    ],
    "homepage": "https://github.com/uchm4n/skeleton-package-package",
    "license": "MIT",
    "authors": [
        {
            "name": "UchMan",
            "email": "ucha19871@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Uchm4n\\SkeletonPackage\\": "src",
            "Uchm4n\\SkeletonPackage\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uchm4n\\SkeletonPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uchm4n\\SkeletonPackage\\SkeletonPackageServiceProvider"
            ],
            "aliases": {
                "SkeletonPackage": "Uchm4n\\SkeletonPackage\\Facades\\SkeletonPackage"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}